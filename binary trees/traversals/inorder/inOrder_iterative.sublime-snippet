	<content><![CDATA[
vector<int> inOrder(Node* root) {
    vector<int> ans;

    stack<Node*> s;
    Node *node = root;

    while (true) {
        if (node) {
            s.push(node);
            node = node->left;
        }
        else {
            if (s.empty()) break;
            node = s.top();
            ans.push_back(node->data);
            s.pop();
            node = node->right;
        }
    }

    return ans;
}
]]></content>
