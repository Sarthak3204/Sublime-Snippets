	<content><![CDATA[
const int N = 1e7;

vector<int> spf(N + 1);
void prime_factor_sieve() {
	fo(i, 1, N) spf[i] = i;

	for (int i = 2; i * i <= N; i++) {
		if (spf[i] == i) {
			for (int j = i * i; j <= N; j += i) spf[j] = i;
		}
	}
}
]]></content>
